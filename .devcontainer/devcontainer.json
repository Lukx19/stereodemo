{
    "name": "stereodeomo",
    "dockerComposeFile": "../docker/docker-compose.yml",
    "service": "stereodemo-dev",
    "workspaceFolder": "/home/user/workspace/",
    "shutdownAction": "stopCompose",
    "customizations": {
      "vscode": {
        "extensions": [
            "076923.python-image-preview",
            "cheshirekow.cmake-format",
            "ms-python.python",
            "ms-python.vscode-pylance",
            "njpwerner.autodocstring",
            "Percy.vscode-numpy-viewer",
            "eamodio.gitlens",
            "mhutchie.git-graph",
            "ms-azuretools.vscode-docker",
            "vivaxy.vscode-conventional-commits"
        ]
      }
    },
    // remove this override if you are not using this devcontainer in Windows WSL
    // if this override is removed on WSL, then the VSCode overrides DISPLAY with :1 which is invalid
    // Whatch this issue to disable automatic X and ssh forwarding https://github.com/microsoft/vscode-remote-release/issues/8031
    "containerEnv": { "DISPLAY": ":0" },
}